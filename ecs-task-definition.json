{
    "family": "nodejs-docker-aws-fargate",
    "containerDefinitions": [
      {
        "name": "node-app",
        "image": "<your-docker-image>",  // Replace with the Docker image URL
        "memory": 512,
        "cpu": 256,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 3000,
            "hostPort": 3000
          }
        ]
      }
    ]
  }
  